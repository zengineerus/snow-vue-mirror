{
  "name": "app",
  "version": "1.0.0",
  "private": true,
  "description": "Front End Snow Vue",
  "author": "WWT",
  "scripts": {
    "build": "node build/build.js",
    "test:unit": "vue-cli-service test:unit",
    "lint": "eslint --ext .js,.vue src test/unit test/e2e/specs",
    "build:android": "node build/build.js : android",
    "build:ios": "node build/build.js : ios",
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js --host 0.0.0.0",
    "docker": "docker run -p 8080:8080 snowvue",
    "docker:build": "docker build -t snowvue .",
    "e2e": "node test/e2e/runner.js",
    "interactive": "docker run -p 8080:8080 -it snowvue /bin/bash",
    "start": "npm run dev",
    "test": "npm run unit && npm run e2e",
    "unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "@vue/cli-service": "^4.1.1",
    "cordova-android": "8.1.0",
    "cordova-ios": "5.1.1",
    "json-server": "^0.15.1",
    "node-sass": "^4.13.0",
    "sass-loader": "7.0.0",
    "vue": "2.6.10",
    "vue-resource": "^1.5.1",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-unit-jest": "^4.1.1",
    "@vue/test-utils": "^1.0.0-beta.30",
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^8.2.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "~8.0.6",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-preset-env": "^1.3.2",
    "babel-register": "^6.22.0",
    "chalk": "^2.0.1",
    "chromedriver": "78.0.1",
    "copy-webpack-plugin": "^4.0.1",
    "cordova-plugin-whitelist": "^1.3.4",
    "cross-env": "^5.0.1",
    "cross-spawn": "^5.0.1",
    "css-loader": "^0.28.0",
    "eslint": "^4.15.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.0.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.4",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.30.1",
    "inject-loader": "^3.0.0",
    "jest": "^24.9.0",
    "nightwatch": "1.3.2",
    "node-notifier": "^5.1.2",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "ora": "^1.2.0",
    "portfinder": "^1.0.13",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.8",
    "postcss-url": "^7.2.1",
    "rimraf": "^2.6.0",
    "selenium-server": "^3.0.1",
    "semver": "^5.3.0",
    "shelljs": "^0.7.6",
    "uglifyjs-webpack-plugin": "^1.1.1",
    "url-loader": "^0.5.8",
    "vue-jest": "^3.0.5",
    "vue-loader": "^13.3.0",
    "vue-server-renderer": "2.6.10",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.5.2",
    "wd": "^1.11.4",
    "webpack": "3.6.0",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-merge": "^4.1.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "../node_modules/babel-jest"
    },
    "preset": "@vue/cli-plugin-unit-jest/presets/default",
    "setupFiles": [
      "./unit-tests.js"
    ]
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {}
    },
    "platforms": [
      "android",
      "ios"
    ]
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  }
}